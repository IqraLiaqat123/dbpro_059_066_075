@model IEnumerable<WebApplication1.Models.Class>

@{
    ViewBag.Title = "listclass";
}

<h2>listclass</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.classname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.classname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.classid }) |
            @Html.ActionLink("Details", "Details", new { id=item.classid }) |
            @Html.ActionLink("Delete", "deleteclass", new { id=item.classid })
        </td>
    </tr>
}

</table>
