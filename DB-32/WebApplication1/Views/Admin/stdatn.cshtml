@model IEnumerable<WebApplication1.Models.Studentattendence>

@{
    ViewBag.Title = "stdatn";
}

<h2>stdatn</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Attendence.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lookup.value)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Attendence.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lookup.value)
        </td>
       
        <td>
            @Html.ActionLink("Back", "studentattendence", new { id=item.Id }) |
            
        </td>
    </tr>
}

</table>
