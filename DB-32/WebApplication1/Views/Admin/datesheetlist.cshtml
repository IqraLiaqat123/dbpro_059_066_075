@model IEnumerable<WebApplication1.Models.Datesheet>

@{
    ViewBag.Title = "datesheetlist";
}

<h2>datesheetlist</h2>

<p>
    @Html.ActionLink("Create New", "adddatesheet")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Class.classname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Class.classname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.title)
        </td>
        <td>
            @Html.ActionLink("Delete", "Deletedatesheet", new { id=item.id })
        </td>
    </tr>
}

</table>
